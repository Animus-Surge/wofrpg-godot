script/source = "extends Button

func _ready():
	$ScrollContainer.visible = false

func _toggled(button_pressed):
	$ScrollContainer.visible = button_pressed
	if !button_pressed:
		var selectedTribes: Array = $ScrollContainer/GridContainer.call(\"getSelectedTribes\")
		if selectedTribes.size() == 0:
			text = \"Tribes\"
		else:
			text = \"\"
			for tribe in selectedTribes:
				text += tribe + \" \"
