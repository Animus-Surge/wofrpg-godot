script/source = "extends GridContainer

const maxSelectedTribes = 3

var selectedTribes

onready var gloader = get_tree().get_root().get_node(\"gloader\")

func _ready():
	for tribe in gloader.loadedtribes:
		var cbox = CheckBox.new()
		cbox.set_script(load(\"res://scripts/ui/checkbox.tres.gd\"))
		cbox.tslotid = tribe.tribeid
		cbox.text = tribe.tribename
		add_child(cbox)

func updateSelection():
	selectedTribes = 0
	for tribe in get_children():
		tribe.disabled = false
		if tribe.pressed:
			selectedTribes += 1
	if selectedTribes >= maxSelectedTribes:
		for tribeUnselected in get_children():
			if !tribeUnselected.pressed:
				tribeUnselected.disabled = true

func getSelectedTribes() -> Array:
	var selected = []
	for tribe in get_children():
		if tribe.pressed:
			selected.append(tribe.tslotid)
	
	return selected

func resetSelected():
	for tribe in get_children():
		tribe.pressed = false
	selectedTribes = 0
"

