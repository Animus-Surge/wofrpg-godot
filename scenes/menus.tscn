[sub_resource type="GDScript" id=2]
script/source = "extends Panel

onready var fsCheck = $CheckBox

func _ready():
	pass
	#TODO: read from settings.json file in user:// directory

func applySettings():
	if fsCheck.pressed:
		OS.window_fullscreen = true
	else:
		OS.window_fullscreen = false
	
	var ratioIndex = $OptionButton.get_selected_id()
	match ratioIndex:
		0: #1920x1080
			OS.window_size = Vector2(1920,1080)
		1: #DEFAULT: 1366x768
			OS.window_size = Vector2(1366,768)
		2: #1440x900
			OS.window_size = Vector2(1440,900)
		3: #1536x864
			OS.window_size = Vector2(1536,864)
		4: #2560x1440
			OS.window_size = Vector2(2560,1440)
		5: #1680x1050
			OS.window_size = Vector2(1680,1050)
		6: #1280x720
			OS.window_size = Vector2(1280,720)
		7: #1280x800
			OS.window_size = Vector2(1280,800)
		8: #1600x900
			OS.window_size = Vector2(1600,900)
"

