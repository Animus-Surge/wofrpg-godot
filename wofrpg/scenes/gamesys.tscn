script/source = "extends Panel

onready var yn = get_node(\"yesno\")
onready var al = get_node(\"alert\")

signal confirmed()
# warning-ignore:unused_signal
signal cancelec()
signal ok()

func _ready():
	hide()

#0 - ERROR, 1 - YES/NO
func showDialogue(title, message, type: int):
	show()
	match type:
		0:
			al.show()
			al.get_node(\"title\").text = title
			al.get_node(\"message\").text = message
		1:
			yn.show()
			yn.get_node(\"title\").text = title
			yn.get_node(\"message\").text = message

func ynConfirm():
	emit_signal(\"confirmed\")
	yn.hide()
	hide()

func ynCancel():
	emit_signal(\"canceled\")
	yn.hide()
	hide()

func alok():
	emit_signal(\"ok\")
	al.hide()
	hide()
"

[sub_resource type="GDScript" id=33]
script/source = "extends Panel

onready var gldr = get_tree().get_root().get_node(\"gloader\")

var chars

func _ready():
# warning-ignore:return_value_discarded
	connect(\"visibility_changed\", self, \"_visibility_changed\")
	$panel/ItemList.clear()
	hide()

func _visibility_changed():
	if visible:
		$panel/Button.disabled = true
		$panel/Button2.disabled = true
		listCharacters()

func listCharacters():
	$panel/ItemList.clear()
	chars = gldr.loadCustom()
	for character in chars:
		$panel/ItemList.add_item(character.cname)

func onBackPressed():
	hide()

func ccharmp():
	var globalvars = get_node(\"/root/globalvars\")
	if globalvars.username != \"\":
		var cdata = chars[$panel/ItemList.get_selected_items()[0]]
		globalvars.setPlayerData(cdata)
		globalvars.setPalette(ImageTexture.new())
		var gl = get_node(\"/root/State\")
		gl.join(\"127.0.0.1\")
	else:
		$dialogue.showDialogue(\"Uh oh!\", \"For some reason... we don't have your username!\", 0)

func ccharPlay():
	var gvars = get_tree().get_root().get_node(\"globalvars\")
	gvars.setPlayerData(chars[$panel/ItemList.get_selected_items()[0]])
	gvars.load_scene(\"res://scenes/testworld.tscn\")

func onListSelected(_index):
	$panel/Button.disabled = false
	var gvars = get_node(\"/root/globalvars\")
	if gvars.loggedIn:
		$panel/Button2.disabled = false
"

