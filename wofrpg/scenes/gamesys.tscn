script/source = "extends Control

var selectedPlayer

onready var globalvars = get_tree().get_root().get_node(\"globalvars\")
onready var fb = get_tree().get_root().get_node(\"fb\")
onready var cfm = get_tree().get_root().get_node(\"cfm\")

func _ready():
	$ui/toolbar/mp.disabled = true
	$ui/toolbar/rmchar.disabled = true
	$ui/toolbar/sp.disabled = true
	$ui/toolbar/save.disabled = true
	$ui/customization.hide()
	globalvars.setCurrentScene(\"gamesys\")
# warning-ignore:return_value_discarded
	get_node(\"ui/cs1\").connect(\"clicked\", self, \"slotPressed\")
# warning-ignore:return_value_discarded
	get_node(\"ui/cs2\").connect(\"clicked\", self, \"slotPressed\")
# warning-ignore:return_value_discarded
	get_node(\"ui/cs3\").connect(\"clicked\", self, \"slotPressed\")
	if globalvars.debug:
		$ui/unamepanel/Label.text = \"DEBUG\"
	else:
		if globalvars.loggedIn:
			$ui/unamepanel/Label.text = globalvars.username
		else:
			$ui/unamepanel.hide()
			$ui/toolbar/mp.disabled = true
		$ui/cs1.call(\"updateName\", cfm.characters[0].name)
		$ui/cs2.call(\"updateName\", cfm.characters[1].name)
		$ui/cs3.call(\"updateName\", cfm.characters[2].name)

func characterSave():
	var charPalettes = get_node(\"ui/character\").getPalettes()
	$ui/customization.hide()
	var charData = { 
		\"name\":get_node(\"ui/customization/TabContainer/Basic Info/cname\").text,
		\"tribes\":get_node(\"ui/customization/TabContainer/Basic Info/tribesbtn/ScrollContainer/GridContainer\").getSelectedTribes(),
		\"role\":get_node(\"ui/customization/TabContainer/Basic Info/rolesbtn/ScrollContainer/GridContainer\").getSelectedRole(),
		\"appearances\":{
			\"head\":get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/head\").get_item_text(get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/head\").selected),
			\"body\":get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/body\").get_item_text(get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/body\").selected),
			\"legs\":get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/legs\").get_item_text(get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/legs\").selected),
			\"tail\":get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/tail\").get_item_text(get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/tail\").selected),
			\"wings\":get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/wings\").get_item_text(get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/wings\").selected),
			\"spine\":get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/spine\").get_item_text(get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/spine\").selected),
			\"tdec\":get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/td\").get_item_text(get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/td\").selected),
			\"sshow\":get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/spinetoggle\").pressed,
			\"eshow\":get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/eyedroptoggle\").pressed,
			\"tshow\":get_node(\"ui/customization/TabContainer/Appearance/Parent/partScroll/GridContainer/tdtoggle\").pressed
		}
	}
	var err = cfm.saveCharacter(selectedPlayer, charData, charPalettes)
	if !err:
		$dialogue.showDialogue(\"ERROR\", \"An error has occoured while trying to save a character.\", 0)
		return
	get_node(\"ui/cs\" + String(selectedPlayer+1) + \"/Label\").text = get_node(\"ui/customization/TabContainer/Basic Info/cname\").text
	$ui/toolbar/sp.disabled = false
	$ui/toolbar/save.disabled = true
	$ui/toolbar/rmchar.disabled = false
	if globalvars.loggedIn:
		$ui/toolbar/mp.disabled = false

func showProfile():
	pass

func cbpressed():
	if globalvars.debug:
		get_tree().quit(0)
	else:
		globalvars.load_scene(\"res://scenes/menus.tscn\")

func resetAll():
	get_node(\"ui/customization/TabContainer/Basic Info/groupGender\").reset()
	get_node(\"ui/customization/TabContainer/Basic Info/tribesbtn/ScrollContainer/GridContainer\").resetSelected()
	get_node(\"ui/customization/TabContainer/Basic Info/rolesbtn/ScrollContainer/GridContainer\").reset()
	get_node(\"ui/customization/TabContainer/Basic Info/cname\").text = \"\"
	get_node(\"ui/character\").reset()

func slotPressed(slot: int):
	resetAll()
	if get_node(\"ui/cs\" + String(slot+1) + \"/Label\").text == \"[empty]\":
		$ui/toolbar/sp.disabled = true
		$ui/toolbar/mp.disabled = true
		$ui/toolbar/save.disabled = false
		$ui/toolbar/rmchar.disabled = true
		get_node(\"ui/customization\").show()
	else:
		var data = cfm.loadCharacter(slot)
		$ui/character.loadDataFromFiles(data)
		$ui/toolbar/sp.disabled = false
		if globalvars.loggedIn:
			$ui/toolbar/mp.disabled = false
		$ui/toolbar/save.disabled = true
		$ui/toolbar/rmchar.disabled = false
		get_node(\"ui/customization\").hide()
	selectedPlayer = slot
	match slot:
		0:
			$ui/cs1.enable()
			$ui/cs2.disable()
			$ui/cs3.disable()
		1:
			$ui/cs1.disable()
			$ui/cs2.enable()
			$ui/cs3.disable()
		2:
			$ui/cs1.disable()
			$ui/cs2.disable()
			$ui/cs3.enable()

func spPressed(): #assumes empty character isn't selected
	var cdata = cfm.loadCharacter(selectedPlayer)
	globalvars.splr = true
	globalvars.setPalette(cdata[1])
	globalvars.setPlayerData(cdata[0])
	globalvars.load_scene(\"res://scenes/testworld.tscn\")

func mpPressed():
	if true:
		$dialogue.showDialogue(\"Oops!\", \"This feature currently is in the works, and therefore not available at this time.\", 0)
		return
	globalvars.splr = false
	if globalvars.username != \"\":
		var cdata = cfm.loadCharacter(selectedPlayer)
		globalvars.setPalette(cdata[1])
		globalvars.setPlayerData(cdata[0])
		var gl = get_node(\"/root/State\")
		gl.join(\"127.0.0.1\")
	else:
		$dialogue.showDialogue(\"Uh oh!\", \"For some reason... we don't have your username!\", 0)

func rmcharPressed():
# warning-ignore:return_value_discarded
	$dialogue.connect(\"confirmed\", self, \"rccnf\")
	$dialogue.showDialogue(\"You sure?\", \"Are you sure you want to delete this character?\", 1)

func rccnf():
	$dialogue.disconnect(\"confirmed\", self, \"rccnf\")
	cfm.deleteChar(selectedPlayer)
	get_node(\"ui/cs\" + String(selectedPlayer+1) + \"/Label\").text = \"[empty]\"
	$ui/toolbar/rmchar.disabled = true
	$ui/toolbar/sp.disabled = true
	$ui/toolbar/mp.disabled = true
	$ui/customization.show()
	$ui/toolbar/save.disabled = false
	resetAll()

func loadCustomCharacter():
	$cchardialogue.show()
"

