var genderIndex

func _ready():
	$fcheck.self_modulate = Color(1, 0.5, 0)
	$mcheck.self_modulate = Color(1, 0.5, 0)

func reset():
	$fcheck.pressed = false
	$mcheck.pressed = false
	$fcheck.disabled = false
	$mcheck.disabled = false

func _mcheck(button_pressed):
	if button_pressed:
		$fcheck.pressed = false
		$fcheck.disabled = false
		$mcheck.disabled = true
		$fcheck.self_modulate = Color(1, 0.5, 0)
		$mcheck.self_modulate = Color(0.5, 1, 1)
		genderIndex = 0

func _fcheck(button_pressed):
	if button_pressed:
		$mcheck.pressed = false
		$mcheck.disabled = false
		$fcheck.disabled = true
		$mcheck.self_modulate = Color(1, 0.5, 0)
		$fcheck.self_modulate = Color(0.5, 1, 1)
		genderIndex = 1

func mcheckHover():
	if !$mcheck.pressed:
		$mcheck.self_modulate = Color(0,1,1)

func mcheckUnhover():
	if !$mcheck.pressed:
		$mcheck.self_modulate = Color(1,0.5,0)

func fcheckHover():
	if !$fcheck.pressed:
		$fcheck.self_modulate = Color(0,1,1)

func fcheckUnhover():
	if !$fcheck.pressed:
		$fcheck.self_modulate = Color(1,0.5,0)
