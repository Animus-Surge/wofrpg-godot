script/source = "extends ScrollContainer

onready var bodydd = $GridContainer/body
onready var wingdd = $GridContainer/wings
onready var taildd = $GridContainer/tail
onready var headdd = $GridContainer/head
onready var legsdd = $GridContainer/legs
onready var spinedd = $GridContainer/spine
onready var tdecdd = $GridContainer/td

onready var edtog = $GridContainer/eyedroptoggle
onready var sptog = $GridContainer/spinetoggle
onready var tdtog = $GridContainer/tdtoggle

onready var gloader = get_tree().get_root().get_node(\"gloader\")

var containsTailDeco = false
var containsEyedrop = false
var containsSpikeDeco = false

func _ready():
	bodydd.add_item(\"Select...\", 255)
	wingdd.add_item(\"Select...\", 255)
	taildd.add_item(\"Select...\", 255)
	headdd.add_item(\"Select...\", 255)
	legsdd.add_item(\"Select...\", 255)
	spinedd.add_item(\"Select...\", 255)
	tdecdd.add_item(\"Select...\", 255)
	
	sptog.visible = false
	edtog.visible = false
	tdtog.visible = false
	tdecdd.visible = false
	get_node(\"GridContainer/Label7\").visible = false

func reset():
	updateSelectedTribes([])

func updateSelectedTribes(tribes):
	bodydd.clear()
	wingdd.clear()
	taildd.clear()
	headdd.clear()
	legsdd.clear()
	spinedd.clear()
	taildd.clear()
	
	sptog.visible = false
	edtog.visible = false
	tdtog.visible = false
	
	get_node(\"GridContainer/Label7\").visible = false
	
	spinedd.visible = false
	tdecdd.visible = false
	
	containsTailDeco = false
	containsEyedrop = false
	containsSpikeDeco = false
	
	bodydd.add_item(\"Select...\", 255)
	wingdd.add_item(\"Select...\", 255)
	taildd.add_item(\"Select...\", 255)
	headdd.add_item(\"Select...\", 255)
	legsdd.add_item(\"Select...\", 255)
	spinedd.add_item(\"Select...\", 255)
	tdecdd.add_item(\"Select...\", 255)
	
# warning-ignore:unused_variable
	var index = 0
	
	for tribeid in tribes:
		for tribeDict in gloader.loadedtribes:
			if tribeDict.tribeid == tribeid:
				bodydd.add_item(tribeDict.tribename, gloader.loadedtribes.find(tribeDict))
				wingdd.add_item(tribeDict.tribename, gloader.loadedtribes.find(tribeDict))
				taildd.add_item(tribeDict.tribename, gloader.loadedtribes.find(tribeDict))
				headdd.add_item(tribeDict.tribename, gloader.loadedtribes.find(tribeDict))
				legsdd.add_item(tribeDict.tribename, gloader.loadedtribes.find(tribeDict))
				for decoflag in tribeDict.decoflags:
					if decoflag == \"spikes\":
						containsSpikeDeco = true
						spinedd.add_item(tribeDict.tribename, gloader.loadedtribes.find(tribeDict))
					elif decoflag == \"eyedrop\":
						containsEyedrop = true
					elif decoflag == \"taildeco\":
						containsTailDeco = true
						tdecdd.add_item(tribeDict.tribename, gloader.loadedtribes.find(tribeDict))
					#TODO: add more of them
		index += 1
	
	bodydd.select(-1)
	wingdd.select(-1)
	taildd.select(-1)
	headdd.select(-1)
	legsdd.select(-1)
	spinedd.select(-1)
	tdecdd.select(-1)
	
	if containsSpikeDeco:
		sptog.visible = true
		sptog.pressed = false
		spinedd.visible = true
	
	if containsEyedrop:
		edtog.visible = true
		edtog.pressed = false
	
	if containsTailDeco:
		tdtog.visible = true
		tdtog.pressed = false
		tdecdd.visible = true
		get_node(\"GridContainer/Label7\").visible = true
"

