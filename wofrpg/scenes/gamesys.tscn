script/source = "extends Panel

var storedCharName
var selected = false
var mouseOver = false

export (int) var slotid = 0

signal clicked(id)

func _ready():
	connect(\"mouse_entered\", self, \"mouseEnter\")
	connect(\"mouse_exited\", self, \"mouseExit\")

func _process(delta):
	if selected:
		if mouseOver:
			self_modulate = Color(0.9, 0.98, 0.48, 0.75)
		else:
			self_modulate = Color(1, 0.8, 0, 0.75)
	else:
		if mouseOver:
			self_modulate = Color(0.9, 0.98, 0.48, 0.75)
		else:
			self_modulate = Color(0.9, 0.98, 0.48, 0.54)

func mouseEnter():
	mouseOver = true

func mouseExit():
	mouseOver = false

func _gui_input(event):
	if !selected:
		if event is InputEventMouseButton:
			if event.button_index == BUTTON_LEFT and event.pressed:
				emit_signal(\"clicked\", slotid)

func updateName(cname):
	$Label.text = cname
	storedCharName = cname

func enable():
	selected = true

func disable():
	selected = false
"

