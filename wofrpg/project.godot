config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="WoFRPG"
config/description="The official Wings of Fire RPG"
run/main_scene="res://scenes/loadscreen.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="WoFRPG"
boot_splash/image="res://programwings-logo_trans.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0, 0.129412, 0.156863, 1 )
config/icon="res://icon.png"
config/macos_native_icon="res://icon-scaled-256p.icns"
config/windows_native_icon="res://favicon.ico"

[autoload]

Test="*res://scripts/test.gd"

[display]

window/size/width=1366
window/size/height=768
window/size/resizable=false
window/size/fullscreen=true
mouse_cursor/custom_image="res://alt-cursor.png"
window/stretch/mode="2d"

[gui]

