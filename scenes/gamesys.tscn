script/source = "extends GridContainer

var roleIndex

func radioPressed(id):
	for child in get_children():
		if child.id != id:
			child.disabled = false
			child.pressed = false
		else:
			child.disabled = true
	
	roleIndex = id
"

[sub_resource type="GDScript" id=15]
script/source = "extends CheckBox

export (int) var id

var mouseOver = false

func _toggled(button_pressed):
	if button_pressed:
		disabled = true
		get_parent().call(\"radioPressed\", id)

func _mouse_enter():
	if !disabled:
		mouseOver = true
		self_modulate = Color(0,1,1)

func _mouse_exit():
	mouseOver = false
	self_modulate = Color(1,0.5,0)

func _process(delta):
	if pressed:
		if !mouseOver:
			self_modulate = Color(0,1,0)
	else:
		if !mouseOver:
			self_modulate = Color(1,0.5,0)
"

