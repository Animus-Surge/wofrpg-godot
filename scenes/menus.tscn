script/source = "extends Control

export (Texture) var body
export (Texture) var wing
export (Texture) var head

var bc
var wc
var hc

var bt
var wt
var tt  #TO BE USED WITH CHARACTER CREATOR
var wit
var ht

func _ready():
	bc = ImageTexture.new()
	bc.create_from_image(body.get_data())
	
	wc = ImageTexture.new()
	wc.create_from_image(wing.get_data())
	
	hc = ImageTexture.new()
	hc.create_from_image(head.get_data())
	
	$spikes.visible = false
	$edrop.visible = false
	
	bt = 0
	wt = 0
	tt = 0
	wit = 0
	ht = 0

func scalesChanged(color):
	var body = bc.get_data()
	var head = hc.get_data()
	var darkened = Color(color.r - 0.08, color.g - 0.08, color.b - 0.08)
	
	body.lock()
	body.set_pixel(0, 0, darkened)
	body.set_pixel(1, 0, color)
	body.unlock()
	
	head.lock()
	head.set_pixel(1,0,color)
	head.unlock()
	
	var h = ImageTexture.new()
	h.create_from_image(head,0)
	h.storage = ImageTexture.STORAGE_COMPRESS_LOSSLESS
	
	var b = ImageTexture.new()
	b.create_from_image(body,0)
	b.storage = ImageTexture.STORAGE_COMPRESS_LOSSLESS
	
	$head.get_material().set_shader_param(\"palette\", h)
	$legs.get_material().set_shader_param(\"palette\", b)
	$tail.get_material().set_shader_param(\"palette\", b)
	$body.get_material().set_shader_param(\"palette\", b)
	
	hc = h
	bc = b

func hornsChanged(color):
	var image = hc.get_data()
	image.lock()
	image.set_pixel(0,0,color)
	image.unlock()
	
	var h = ImageTexture.new()
	h.create_from_image(image,0)
	h.storage = ImageTexture.STORAGE_COMPRESS_LOSSLESS
	
	$head.get_material().set_shader_param(\"palette\", h)
	
	hc = h

func eyesChanged(color):
	var image = hc.get_data()
	image.lock()
	image.set_pixel(2,0,color)
	image.unlock()
	
	var h = ImageTexture.new()
	h.create_from_image(image,0)
	h.storage = ImageTexture.STORAGE_COMPRESS_LOSSLESS
	$head.get_material().set_shader_param(\"palette\", h)
	
	hc = h

func wingsChanged(color):
	var image = wc.get_data()
	var darkened = Color(color.r - 0.08, color.g - 0.08, color.b - 0.08)
	
	image.lock()
	image.set_pixel(0,0,darkened)
	image.set_pixel(1,0,color)
	image.unlock()
	
	var w = ImageTexture.new()
	w.create_from_image(image,0)
	w.storage = ImageTexture.STORAGE_COMPRESS_LOSSLESS
	$wings.get_material().set_shader_param(\"palette\",w)
	
	wc = w

func spikesChanged(color):
	$spikes.self_modulate = color

func bodyTypeChanged(index):
	match index:
		globalvars.TRIBES_BUILTIN.NIGHTWING:
			$body.texture = load(\"res://images/character/night/idle/nw-body.png\")
			$body.get_material().set_shader_param(\"mask\", load(\"res://images/character/night/idle/nw-body-mask.png\"))
		globalvars.TRIBES_BUILTIN.SKYWING:
			$body.texture = load(\"res://images/character/sky/idle/sw-body.png\")
			$body.get_material().set_shader_param(\"mask\", load(\"res://images/character/sky/idle/sw-body-mask.png\"))

func headTypeChanged(index):
	match index:
		globalvars.TRIBES_BUILTIN.NIGHTWING:
			$head.texture = load(\"res://images/character/night/idle/nw-head.png\")
			$head.get_material().set_shader_param(\"mask\", load(\"res://images/character/night/idle/nw-head-mask.png\"))
		globalvars.TRIBES_BUILTIN.SKYWING:
			$head.texture = load(\"res://images/character/sky/idle/sw-head.png\")
			$head.get_material().set_shader_param(\"mask\", load(\"res://images/character/sky/idle/sw-head-mask.png\"))

func wingTypeChanged(index):
	match index:
		globalvars.TRIBES_BUILTIN.NIGHTWING:
			$wings.texture = load(\"res://images/character/night/idle/nw-wings.png\")
			$wings.get_material().set_shader_param(\"mask\", load(\"res://images/character/night/idle/nw-wings-mask.png\"))
		globalvars.TRIBES_BUILTIN.SKYWING:
			$wings.texture = load(\"res://images/character/sky/idle/sw-wings.png\")
			$wings.get_material().set_shader_param(\"mask\", load(\"res://images/character/sky/idle/sw-wings-mask.png\"))

func tailTypeChanged(index):
	match index:
		globalvars.TRIBES_BUILTIN.NIGHTWING:
			$tail.texture = load(\"res://images/character/night/idle/nw-tail.png\")
			$tail.get_material().set_shader_param(\"mask\", load(\"res://images/character/night/idle/nw-tail-mask.png\"))
		globalvars.TRIBES_BUILTIN.SKYWING:
			$tail.texture = load(\"res://images/character/sky/idle/sw-tail.png\")
			$tail.get_material().set_shader_param(\"mask\", load(\"res://images/character/sky/idle/sw-tail-mask.png\"))

func legsTypeChanged(index):
	match index:
		globalvars.TRIBES_BUILTIN.NIGHTWING:
			$legs.texture = load(\"res://images/character/night/idle/nw-legs.png\")
			$legs.get_material().set_shader_param(\"mask\", load(\"res://images/character/night/idle/nw-legs-mask.png\"))
		globalvars.TRIBES_BUILTIN.SKYWING:
			$legs.texture = load(\"res://images/character/sky/idle/sw-legs.png\")
			$legs.get_material().set_shader_param(\"mask\", load(\"res://images/character/sky/idle/sw-legs-mask.png\"))

func spikesToggled(button_pressed):
	$spikes.visible = button_pressed

func _on_tdscalevis_toggled(button_pressed):
	$edrop.visible = button_pressed

func playAnimsToggle(button_pressed):
	if button_pressed:
		$AnimationPlayer.play(\"prev-idle\")
	else: 
		$AnimationPlayer.stop()
"

