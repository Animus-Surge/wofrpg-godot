script/source = "extends Panel

func colorChanged(color: Color, part: int):
	match part:
		0: #scales
			get_node(\"../../../character\").call(\"scalesChangeC\", color)
		1: #eyes
			get_node(\"../../../character\").call(\"eyesChangeC\", color)
		2: #horns
			get_node(\"../../../character\").call(\"hornsChangeC\", color)
		3: #wings
			get_node(\"../../../character\").call(\"wingsChangeC\", color)
		4: #spine
			get_node(\"../../../character\").call(\"spineChangeC\", color)
		5: #tail decoration
			get_node(\"../../../character\").call(\"tdecChangeC\", color)

func reset():
	$Parent/colorScroll/GridContainer/eyes.color = Color.white
	$Parent/colorScroll/GridContainer/scales.color = Color.white
	$Parent/colorScroll/GridContainer/horns.color = Color.white
	$Parent/colorScroll/GridContainer/wings.color = Color.white
	$Parent/colorScroll/GridContainer/spine.color = Color.white
	$Parent/colorScroll/GridContainer/tdec.color = Color.white
	$Parent/partScroll.reset()
"

