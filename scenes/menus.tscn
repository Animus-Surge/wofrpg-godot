script/source = "extends HBoxContainer

const nohov = preload(\"res://images/ui/buttons/mmbutton-graphic-blank.png\")
const hover = preload(\"res://images/ui/buttons/mmbutton-graphic.png\")
const click = preload(\"res://images/ui/buttons/mmbutton-graphic-click.png\")

func _ready():
# warning-ignore:unsafe_property_access
	get_node(\"TextureRect\").texture = nohov

func mouseOver():
# warning-ignore:unsafe_property_access
	get_node(\"TextureRect\").texture = hover

func mouseExit():
# warning-ignore:unsafe_property_access
	get_node(\"TextureRect\").texture = nohov

func _gui_input(event):
	if event is InputEventMouseButton:
		if event.button_index == BUTTON_LEFT and event.pressed:
# warning-ignore:unsafe_property_access
			get_node(\"TextureRect\").texture = click
# warning-ignore:unsafe_method_access
			get_parent().get_parent().get_parent().get_parent().get_parent().get_parent()._show(name)
		else:
# warning-ignore:unsafe_property_access
			get_node(\"TextureRect\").texture = hover
"

